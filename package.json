  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "//": "New scripts to run and build Storybook with Tailwind",
    "storybook": "concurrently \"npm:watch:*\"",
    "build-storybook": "concurrently \"npm:build:*\"",
    "build:css": "npx tailwindcss -i ./src/styles/global.css",
    "build:storybook": "build-storybook",
    "watch:css": "npx tailwindcss -i ./src/styles/global.css --watch",
    "watch:storybook": "start-storybook -p 6006"
  },
